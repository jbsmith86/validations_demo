<h1>Dogs</h1>
<table>
  <tr>
    <th>Name</th>
    <th>Actions</th>
  </tr>
  <% @dogs.each do |dog| %>
    <tr>
      <td><%= dog.name %></td>
      <td>
        <%= link_to "Show", dog_path(dog) %> |
        <%= link_to "Edit", edit_dog_path(dog) %> |
        <%= link_to "Delete", dog_path(dog), method: :delete, data: { confirm: 'Are you sure?'} %>
      </td>
    </tr>
  <% end %>
</table>


<%= link_to 'New Dog', new_dog_path %>